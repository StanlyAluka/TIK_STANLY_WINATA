#include <iostream>
#include <math.h>
using namespace std;

int main (){
    //operator aritmatika
    int a = 80;
    int b = 9;
    
    int hasiltambah = a + b;
    int hasilkurang = a - b;
    int hasilbagi = a / b;
    int hasilkali = a * b;
    int sisabagi = a % b;
    
    
    
    
    cout<<hasiltambah<<endl;
    cout<<hasilkurang<<endl;
    cout<<hasilbagi<<endl;
    cout<<hasilkali<<endl;
    cout<<sisabagi<<endl;
    
    
    //int = bilangan bulat
    //float = bilangan desimal
    
    
    return 0;
}
